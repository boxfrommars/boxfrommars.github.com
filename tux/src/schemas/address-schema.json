{
    "id": "/address",
    "title": "Address Schema",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "isPublic": {
            "type": "boolean"
        },
        "street": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            },
            "required": ["id", "name"]
        },
        "district": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "city": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "integer"
                        },
                        "name": {
                            "type": "string"
                        }
                    },
                    "required": ["id", "name"]
                }
            },
            "required": ["id", "name", "city"]
        },
        "house": {
            "type": "string"
        },
        "entrance": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "location": {
                    "$ref": "/geopoint"
                }
            }
        },
        "description": {
            "type": "string"
        },
        "location": {
            "$ref": "/geopoint"
        }
    },
    "required": ["id", "isPublic", "street", "district", "house", "entrance", "description"]
}